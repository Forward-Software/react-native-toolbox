{
  "name": "@panz3r/react-native-toolbox",
  "version": "2.1.0",
  "description": "A set of scripts to simplify React Native development",
  "author": {
    "name": "Mattia Panzeri",
    "email": "mattia.panzeri93@gmail.com",
    "url": "https://github.com/panz3r"
  },
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panz3r/react-native-toolbox.git"
  },
  "keywords": [
    "react-native",
    "scripts",
    "utils",
    "development"
  ],
  "homepage": "https://github.com/panz3r/react-native-toolbox#readme",
  "bugs": {
    "url": "https://github.com/panz3r/react-native-toolbox/issues"
  },
  "funding": "https://buymeacoff.ee/4f18nT0Nk",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "rn-toolbox",
    "dirname": "rn-toolbox",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "cleanup": "shx rm -rf android/ ios/ dist/ .nyc_output/ oclif.manifest.json .env",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "build": "shx rm -rf dist && tsc -b",
    "version": "oclif readme && git add README.md",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json"
  },
  "bin": {
    "rn-toolbox": "./bin/run"
  },
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "chalk": "^4.0.0",
    "listr": "^0.14.3",
    "sharp": "^0.30.3"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.23",
    "@types/sharp": "^0.30.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "nyc": "^14",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
